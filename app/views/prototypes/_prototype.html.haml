.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(prototype.images[0][:image]), prototype_path(prototype)
    .caption
      %h3
        = prototype.name
      .proto-meta
        .proto-user
          = link_to "by #{prototype.user.name}", root_path
        .proto-posted
          Apr 26
      %ul.proto-tag-list.list-inline
        %li 
          = link_to "iPad", root_path, class: "btn btn-default"
        %li 
          = link_to "wireframe", root_path, class: "btn btn-default"

